# OpenAI Configuration (Primary Model)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini
OPENAI_TIMEOUT_MS=60000

# Groq Configuration (Optional - For Fast Draft Generation)
# Get your free API key at https://console.groq.com
# Enables two-stage optimization: ~300ms draft + background refinement
GROQ_API_KEY=gsk_your_groq_api_key_here
GROQ_MODEL=llama-3.1-8b-instant
GROQ_TIMEOUT_MS=5000

# Firebase Configuration (Client)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Google Cloud Storage (prod-only; no local storage fallback)
# Bucket name can be a raw bucket (e.g. "my-bucket"), "gs://my-bucket", or a storage URL.
GCS_BUCKET_NAME=your-prod-bucket-name
IMAGE_STORAGE_BUCKET=your-prod-bucket-name
VIDEO_STORAGE_BUCKET=your-prod-bucket-name
ASSET_STORAGE_BUCKET=your-prod-bucket-name
# Optional explicit location for span-labeling model assets.
# Example: gs://your-prod-bucket-name/span-labeling/models/
SPAN_LABELING_MODELS_GCS_URI=

# Service account JSON path (required for server-side GCS access unless running in GCP with workload identity)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# Server Configuration
PORT=3001
NODE_ENV=development

# Prompt Output Test Mode (disable video services + GLiNER)
PROMPT_OUTPUT_ONLY=false

# Logging Configuration
# Backend log level: debug, info, warn, error
# Defaults: debug (development), info (production)
LOG_LEVEL=info

# Provider Health (Optional)
# Disable Gemini if health check fails (recommended for reliability)
GEMINI_ALLOW_UNHEALTHY=false

# Startup Validation (Optional - defaults to lazy)
SERVICE_STARTUP_HEALTHCHECKS=false
SERVICE_STARTUP_PRE_RESOLVE=false

# LLM Concurrency Limits (Optional)
OPENAI_MAX_CONCURRENT=5
OPENAI_QUEUE_TIMEOUT_MS=30000
GROQ_MAX_CONCURRENT=5
GROQ_QUEUE_TIMEOUT_MS=30000
GEMINI_MAX_CONCURRENT=5
GEMINI_QUEUE_TIMEOUT_MS=30000

# Frontend Logging Configuration
# Enable debug logging in production (defaults to enabled in development)
VITE_DEBUG_LOGGING=true

# Frontend log level: debug, info, warn, error
# Defaults: debug (development), warn (production)
VITE_LOG_LEVEL=debug

# API Authentication (Optional - for production)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
API_KEY=your_api_key_here

# Redis Configuration (Optional - for caching)
REDIS_URL=redis://localhost:6379

# Stripe Configuration (for billing)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Stripe Price ID to Credits Mapping
# Maps Stripe priceIds to credit amounts granted on purchase
# Format: priceId=credits,priceId=credits,...
# Subscriptions:
#   price_explorer_monthly=500
#   price_creator_monthly=1800
#   price_agency_monthly=6000
# Credit Packs (one-time):
#   price_credits_250=300
#   price_credits_500=600
#   price_credits_1000=1200
#   price_credits_2500=3000
STRIPE_PRICE_CREDITS=price_explorer_monthly=500,price_creator_monthly=1800,price_agency_monthly=6000,price_credits_250=300,price_credits_500=600,price_credits_1000=1200,price_credits_2500=3000

# Replicate Configuration (fallback for depth estimation, plus image preview generation)
# Get your API token at https://replicate.com/account/api-tokens
REPLICATE_API_TOKEN=r8_your_replicate_api_token_here

# Fal.ai Configuration (depth estimation + PuLID face identity)
# Get your API key at https://fal.ai/dashboard/keys
# Used for depth estimation and face-consistent keyframe generation
FAL_KEY=your_fal_api_key_here

# Luma Configuration (for video generation)
LUMA_API_KEY=your_luma_api_key_here
# Alternative env var supported by Luma SDK:
# LUMAAI_API_KEY=your_luma_api_key_here

# Sentry Error Tracking (Optional but Recommended)
# Get your DSN from: https://sentry.io/settings/projects/
SENTRY_DSN=https://your_sentry_dsn_here@sentry.io/project_id

# Sentry Configuration (Optional)
VITE_SENTRY_DSN=https://your_sentry_dsn_here@sentry.io/project_id
VITE_ENVIRONMENT=development
VITE_APP_VERSION=1.0.0

# Sentry Source Maps Upload (Production builds only)
SENTRY_ORG=your_sentry_org
SENTRY_PROJECT=your_sentry_project
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# Build Source Maps (Optional)
GENERATE_SOURCEMAP=false

# Enable Sentry in development (Optional - defaults to disabled)
SENTRY_DEBUG=false
VITE_SENTRY_DEBUG=false
